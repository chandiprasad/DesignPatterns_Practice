package com.practice.ds.alogs;

public class Programme1 {

	public static void main(String[] args) {
		
		simpleFunction(1000);
	}
	
	public static void simpleFunction(int n){
		int a = 9;
		int b = 3;
		
		int sum = a +b +n;
		int product = a*b*n;
		int quotient = a* n/b;
		
		System.out.println(String.format("The sume is : %s, product is : %s", sum,product,quotient));
		
	}

}
